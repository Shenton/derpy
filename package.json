    "chartjs-plugin-colorschemes": "^0.4.0",
    "connect-mongodb-session": "^2.4.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "discord.js": "^12.3.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-session": "^1.17.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.1.1",
    "html-specialchars": "^1.0.4",
    "humanize-duration": "^3.23.1",
    "jsonfile": "^6.0.1",
    "moment": "^2.28.0",
    "mongoose": "^5.10.5",
    "morgan": "^1.10.0",
    "node-opus": "^0.3.3",
    "node-sass": "^4.14.1",
    "nuxt": "^2.14.5",
    "nuxt-helmet": "^2.0.0",
    "path": "^0.12.7",
    "pm2": "^4.4.1",
