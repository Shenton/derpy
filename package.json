{
  "name": "derpy",
  "version": "2.2.2",
  "description": "Derpy the discord bot",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon app.js",
    "build": "nuxt build -c ./web/nuxt.config.js",
    "start": "cross-env NODE_ENV=production pm2 start app.js",
    "stop": "pm2 stop all && pm2 kill",
    "restart": "npm stop && npm start",
    "list": "pm2 list",
    "monit": "pm2 monit",
    "pm2logrotate": "pm2 install pm2-logrotate"
  },
  "dependencies": {
